<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls1="clr-namespace:Tile.Controls;assembly=Tile"
             x:Name="Main"
             x:Class="Tile.MainPage">
    <StackLayout>

        <Button Text="HorizontalTilesPage"
                Margin="10"
                x:Name="HorizontalTilesPage"
                BackgroundColor="#EEF3F6"
                Clicked="HorizontalTilesPage_Clicked"></Button>

        <Button Text="GridTilesPage"
                Margin="10"
                x:Name="GridTilesPage"
                BackgroundColor="#EEF3F6"
                Clicked="GridTilesPage_Clicked"></Button>


    </StackLayout>

</ContentPage>
